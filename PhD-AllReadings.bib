	Doi = {10.3389/fevo.2019.00123},
	Issn = {2296-701X},
	Journal = {Frontiers in Ecology and Evolution},
	Pages = {123},
	Read = {1},
	Title = {Apparent Competition, Lion Predation, and Managed Livestock Grazing: Can Conservation Value Be Enhanced?},
